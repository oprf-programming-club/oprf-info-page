{
  "name": "oprf-info-page",
  "version": 2,
  "alias": "https://oprf-info-page.now.sh",
  "build": {
    "env": {
      "OPRF_API_ANALYTICS_URL": "@oprf-api-analytics-url"
    }
  },
  "builds": [
    {
      "src": "api/now-runner.js",
      "use": "@now/node"
    },
    {
      "src": "app/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/now-runner.js"
    },
    {
      "src": "/(.*)",
      "dest": "app/$1"
    }
  ],
  "github": {
    "silent": true
  }
}
